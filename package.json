{
  "name": "typed-dom",
  "version": "0.0.305",
  "description": "A value-level and type-level DOM builder.",
  "private": true,
  "homepage": "https://github.com/falsandtru/typed-dom",
  "repository": {
    "type": "git",
    "url": "https://github.com/falsandtru/typed-dom"
  },
  "keywords": [
    "type",
    "dom",
    "shadow dom",
    "custom element",
    "css",
    "css-in-js",
    "web component"
  ],
  "types": "./index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "index.*",
    "src",
    "test",
    "*.ts",
    "*.js",
    "*.json",
    "*.map",
    "*.md",
    "NOTICE",
    "LICENSE"
  ],
  "dependencies": {
    "typed-query-selector": "^2.7.0"
  },
  "devDependencies": {
    "@types/benchmark": "2.1.2",
    "@types/i18next": "13.0.0",
    "@types/mocha": "9.1.1",
    "@types/power-assert": "1.5.8",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-unassert": "^3.2.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.0",
    "eslint-plugin-redos": "^4.4.1",
    "eslint-webpack-plugin": "^3.2.0",
    "glob": "^8.0.3",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-power-assert": "^1.0.0",
    "mocha": "^10.0.0",
    "npm-check-updates": "^16.1.1",
    "spica": "0.0.622",
    "ts-loader": "^9.3.1",
    "typescript": "4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "update": "ncu -u && npm i --no-shrinkwrap",
    "dev": "concurrently \"webpack --env mode=test -w\" \"karma start --auto-watch\"",
    "test": "webpack --env mode=test && karma start --single-run --concurrency 1 --reporters dots,coverage",
    "lint": "webpack --env mode=lint",
    "bench": "webpack --env mode=bench && karma start --single-run --concurrency 1 --browsers Chrome",
    "dist": "webpack --env mode=dist"
  },
  "author": "falsandtru",
  "license": "(Apache-2.0 AND MPL-2.0)"
}
